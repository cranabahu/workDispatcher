template(name="photo")
   #page-wrapper
      .row
         .col-lg-12
            h1.page-header Attachments
      .row
         .col-lg-12
            .panel.panel-default
               .panel-heading
                  i.fa.fa-camera.fa-fw
                  label(style="color:green;font-size:150%") Images
               .panel-body
                  .row
                     .col-lg-6
                        each photos
                           .row(style="position:relative;padding:5px;margin-left:40%")
                              img(src='#{picture}')